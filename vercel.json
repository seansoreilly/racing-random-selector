{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "styles.css",
            "use": "@vercel/static"
        },
        {
            "src": "script.js",
            "use": "@vercel/static"
        },
        {
            "src": "sprites/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/styles.css",
            "dest": "/styles.css"
        },
        {
            "src": "/script.js",
            "dest": "/script.js"
        },
        {
            "src": "/sprites/(.*)",
            "dest": "/sprites/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}